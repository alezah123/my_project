package emailapp;

public class Email {

	public static void main(String[] args) {
		
		EmailApp em1 = new EmailApp("Stan","Smith");
		
		System.out.println();
		
		System.out.println(em1.getName());
		
		System.out.println(em1.getEmail());
		
		System.out.println(em1.getPassword());
		
		System.out.println(em1.getMailboxCapacity());
		
		em1.setPassword("BoomShakaLaka");
		
		System.out.println("The new password is: " + em1.getPassword());
		
	}

}
